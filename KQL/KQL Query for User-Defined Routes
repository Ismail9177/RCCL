resources
| where type == "microsoft.network/virtualnetworks"
| mv-expand subnets = properties.subnets
| project
    subscriptionId,
    resourceGroup,
    vnetName = name,
    subnetName = tostring(subnets.name),
    addressPrefix = tostring(subnets.properties.addressPrefix),
    provisioningState = tostring(subnets.properties.provisioningState),
    routeTableId = tostring(subnets.properties.routeTable.id)
